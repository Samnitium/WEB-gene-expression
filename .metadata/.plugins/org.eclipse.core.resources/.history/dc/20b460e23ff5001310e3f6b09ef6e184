<?php
	class Analysis {
	
		var $id;
		var $geneSymbol;
		var $p_value;
		var $foldChange;
		var $name;
		var $data;
		var $id_esperimento;
		
		
		public function __construct() {
			$this->id = null;
			$this->email = null;
			$this->password = null;
			$this->name = null;
			$this->surname = null;
			$this->type = null;
		}
	
	public function __get($value) {
		return $this->$value;
	}
	
	public function __set($name,$value) {
		$this->$name = $value;
	}
	
	
		
}
	
	
	

?>